    "PanelEntityTemplateSchema": {
      "type": "object",
      "description": "Represents the serializable schema for a panel object template\n            ",
      "x-abstract": true,
      "additionalProperties": false,
      "properties": {
        "AbilityLevel": {
          "type": "integer",
          "description": "The ability level required to use this entity\n            ",
          "format": "int32"
        },
        "AdvClass": {
          "description": "Defaults to null\n\nIf specified, this is the advanced class required to use this entity\n            ",
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/definitions/AdvClass"
            }
          ]
        },
        "Class": {
          "description": "Defaults to null\n\nIf specified, this is the base class required to use this entity\n            ",
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/definitions/BaseClass"
            }
          ]
        },
        "CooldownMs": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Defaults to null\n\nIf specified, any on-use effect of this object will use this cooldown\n            ",
          "format": "int32"
        },
        "Description": {
          "type": [
            "null",
            "string"
          ],
          "description": "Defaults to null\n\nA brief description of this entity\n            "
        },
        "Level": {
          "type": "integer",
          "description": "The level required to use this entity\n            ",
          "format": "int32"
        },
        "Name": {
          "type": "string",
          "description": "The base name of the entity\n            "
        },
        "PanelSprite": {
          "type": "integer",
          "description": "The sprite id used to display the entity in it's respective panel (no offset)\n            "
        },
        "RequiresMaster": {
          "type": "boolean",
          "description": "Whether or not this entity requires you to be a master\n            "
        },
        "ScriptKeys": {
          "type": "array",
          "description": "A collection of names of scripts to attach to this object by default. The name of a script is just the name of the\nclass without \"Script\" at the end.\n            ",
          "items": {
            "type": "string"
          }
        },
        "ScriptVars": {
          "type": "object",
          "description": "A collection of key-value pairs of key-value pairs\n\nEach script that has variables needs a scriptName-Value pair, and the value of that entry is a dictionary of\npropertyName-Value pairs\n            ",
          "additionalProperties": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/KeyValuePairOfStringAndJsonElement"
            }
          }
        },
        "TemplateKey": {
          "type": "string",
          "description": "A unique id specific to this template. This must match the file name\n            "
        }
      }
    },
    "AdvClass": {
      "type": "string",
      "description": "A byte representing the 'medenia class' of an aisling. Used in many places.\n            ",
