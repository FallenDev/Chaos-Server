  "allOf": [
    {
      "$ref": "#/definitions/PanelEntityTemplateSchema"
    },
    {
      "type": "object",
      "description": "Represents the serializable schema for an item template\n            ",
      "additionalProperties": false,
      "properties": {
        "AccountBound": {
          "type": "boolean",
          "description": "Whether or not the item is account bound. If the item is account bound, it cannot be traded or dropped\n            "
        },
        "ArmorUsesOvercoatSprites": {
          "type": "boolean",
          "description": "When this item is equipped, it will be considered an overcoat for display purposes. This allows items in the normal\narmor slot to use overcoat sprites and be dyeable. If an overcoat is equipped, that item will show instead.\n            "
        },
        "BuyCost": {
          "type": "integer",
          "description": "The amount of gold it costs to buy this item from a merchant\n            ",
          "format": "int32"
        },
        "Category": {
          "type": "string",
          "description": "The category of the item, used for bank or shop sorting\n            "
        },
        "Color": {
          "description": "Defaults to None(lavender)\n\nThe color of the item\n            ",
          "oneOf": [
            {
              "$ref": "#/definitions/DisplayColor"
            }
          ]
        },
        "DisplaySprite": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Default null\n\nIf specified, this is the sprite value used to display the item on the character when equipped\n            "
        },
        "EquipmentType": {
          "description": "Default null\n\nIf specified, this is the type of equipment this item is\n            ",
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/definitions/EquipmentType"
            }
          ]
        },
        "Gender": {
          "description": "Default null\n\nIf specified, this is the gender required to use this item\n            ",
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/definitions/Gender"
            }
          ]
        },
        "IsDyeable": {
          "type": "boolean",
          "description": "Whether or not the item can be dyed. This determines whether a color change applied to an item persists through\nserialization and deserialization. Town dyes on pre-99 armor is not dye, theyre completely different sprites.\n            "
        },
        "IsModifiable": {
          "type": "boolean",
          "description": "Whether or not an instance of the item can be modified. This controls if an item can be affected by meta node\nmutators.\n            "
        },
        "MaxDurability": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Defaults to null\n\nIf specified, the max durability of the item\n            ",
          "format": "int32"
        },
        "MaxStacks": {
          "type": "integer",
          "description": "The maximum number of this item that can be in a stack. If set to 1, the item will not be stackable\n            ",
          "format": "int32"
        },
        "Modifiers": {
          "description": "Defaults to null\n\nIf specified, these are the stats this item grants when equipped\n            ",
          "oneOf": [
            {
              "type": "null"
            },
            {
              "$ref": "#/definitions/AttributesSchema"
            }
          ]
        },
        "NoTrade": {
          "type": "boolean",
          "description": "Whether or not the item can be traded. If the item is not tradeable, it cannot be traded to other players via\nexchange or dropping. These items are still droppable, but are locked to the player that dropped it.\n            "
        },
        "OvercoatUsesArmorSprites": {
          "type": "boolean",
          "description": "When this item is equipped, it will be considered an armor for display purposes. This allows items in the overcoat\nslot to use normal armor sprites. If both an armor and an overcoat are equipped, the overcoat will display.\n            "
        },
        "OverridesBootsSprite": {
          "type": "boolean",
          "description": "When this item is equipped, the boots sprite will be set to 0\n            "
        },
        "OverridesHeadSprite": {
          "type": "boolean",
          "description": "When this item is equipped, the head sprite will be set to 0\n            "
        },
        "PantsColor": {
          "description": "Default null\n\nIf specified, this armor will have pants, and they will be this color\n            ",
          "oneOf": [
            {
              "$ref": "#/definitions/DisplayColor"
            }
          ]
        },
        "PreventBanking": {
          "type": "boolean",
          "description": "Whether or not the item can be banked\n            "
        },
        "SellValue": {
          "type": "integer",
          "description": "The amount of gold given for selling this item to a merchant\n            ",
          "format": "int32"
        },
        "Weight": {
          "type": "integer",
          "description": "The weight of the item in the inventory, or equipped\n            ",
          "format": "byte"
        }
      }
    }
  ]
}